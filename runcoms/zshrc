# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

# alias and other things
eval "$(hub alias -s)"
eval "$(fasd --init auto)"

export VISUAL='nvim'
export TERM="xterm-256color"
export PATH=~/.local/bin:$PATH
alias v='f -e nvim'
alias vim="nvim"
alias sudo="sudo "
